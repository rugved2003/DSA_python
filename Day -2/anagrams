def anagrams (s1,s2):
    dict = {}
    
    for char in s1:
        if char in dict:
            dict[char] += 1
        else:
            dict[char] = 1
        
    for char in s2:
        if char in dict:
            dict[char] -= 1
        else:
            return 'False'
            
    for value in dict.values():
        if value !=0:
            return 'False'
            
    return 'True'
    
s1 = str(input())
s2 = str(input())
print(anagrams(s1,s2))
            
        
